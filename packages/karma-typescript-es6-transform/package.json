{
  "name": "karma-typescript-es6-transform",
  "version": "5.5.2",
  "description": "Plugin for transforming ES2015 javascript code to to ES5 syntax.",
  "files": [
    "dist/**"
  ],
  "main": "dist/transform.js",
  "keywords": [
    "babel",
    "ES2015",
    "ES6",
    "karma-typescript",
    "typescript"
  ],
  "author": "monounity",
  "contributors": [
    "erikbarke"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/monounity/karma-typescript.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "nyc --require ts-node/register tape \"src/*.spec.ts\" | faucet"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "reporter": [
      "html"
    ],
    "tempDir": "./coverage/.nyc_output"
  },
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "acorn": "^8.1.0",
    "acorn-walk": "^8.0.2",
    "log4js": "^6.3.0",
    "magic-string": "^0.25.7"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.9",
    "@types/karma": "^6.1.0",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/sinon": "^9.0.4",
    "@types/tape": "^4.13.0",
    "faucet": "0.0.1",
    "i": "^0.3.6",
    "karma-typescript": "5.5.2",
    "nyc": "^15.1.0",
    "postcss": "^8.2.10",
    "sinon": "^9.0.3",
    "tape": "^5.0.1",
    "ts-node": "^8.10.2",
    "typescript": "latest"
  }
}
