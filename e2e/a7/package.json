{
  "name": "a7",
  "description": "Angular 7",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "npm": "npm",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng-packagr -c ./tsconfig.json",
    "test": "npm run test:jasmine && npm run test:jest",
    "test:debug": "npm run test:jasmine:es2015 -- --browsers=Chrome --watch",
    "test:jasmine": "npm run test:jasmine:es5 && npm run test:jasmine:es2015",
    "test:jasmine:es5": "ng test --ts-config ./tsconfig.es5.spec.json --progress=false",
    "test:jasmine:es2015": "ng test --ts-config ./tsconfig.es2015.spec.json --progress=false",
    "test:jasmine:debug": "ng test -- --watch --browsers Chrome",
    "test:jest": "npm run test:jest:es5 && npm run test:jest:es2015",
    "test:jest:es5": "jest -i --config ./jest.es5.js",
    "test:jest:es2015": "jest -i --config ./jest.es2015.js",
    "test:jest:debug": "jest -i --watch"
  },
  "peerDependencies": {
    "ng-mocks": "*"
  },
  "dependencies": {
    "@angular/animations": "7.2.16",
    "@angular/common": "7.2.16",
    "@angular/compiler": "7.2.16",
    "@angular/core": "7.2.16",
    "@angular/forms": "7.2.16",
    "@angular/platform-browser": "7.2.16",
    "@angular/platform-browser-dynamic": "7.2.16",
    "@angular/router": "7.2.16",
    "core-js": "2.6.12",
    "rxjs": "6.3.3",
    "tslib": "1.14.1",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.10",
    "@angular/cli": "7.3.10",
    "@angular/compiler-cli": "7.2.16",
    "@angular/language-service": "7.2.16",
    "@types/jasmine": "2.8.22",
    "@types/jest": "24.9.1",
    "@types/node": "8.10.66",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.9.0",
    "jest-preset-angular": "7.1.1",
    "karma": "5.2.3",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-packagr": "5.7.1",
    "npm": "6.14.18",
    "puppeteer": "2.1.1",
    "ts-jest": "24.3.0",
    "ts-node": "7.0.1",
    "typescript": "3.2.4"
  },
  "engines": {
    "npm": "6.14.18"
  }
}
