    "@angular/platform-browser-dynamic": "20.3.16",
    "@angular/router": "20.3.16",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.3.16",
    "@angular/cli": "20.3.16",
    "@angular/compiler-cli": "20.3.16",
    "@types/jasmine": "6.0.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.13",
