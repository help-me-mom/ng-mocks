    "tslib": "2.7.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.1",
    "@angular/cli": "18.2.1",
    "@angular/compiler-cli": "18.2.2",
