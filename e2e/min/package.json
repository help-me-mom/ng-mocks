    "@angular/platform-browser-dynamic": "20.0.7",
    "@angular/router": "20.0.7",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.3.13",
    "@angular/cli": "20.3.13",
    "@angular/compiler-cli": "20.0.7",
    "@types/jasmine": "5.1.13",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.4",
