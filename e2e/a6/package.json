{
  "name": "a6",
  "description": "Angular 6",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "npm": "npm",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng-packagr -c ./tsconfig.json",
    "test": "npm run test:jasmine && npm run test:jest",
    "test:debug": "npm run test:jasmine:es2015 -- --browsers=Chrome --watch",
    "test:jasmine": "npm run test:jasmine:es5 && npm run test:jasmine:es2015",
    "test:jasmine:es5": "ng test --ts-config ./tsconfig.es5.spec.json --progress=false",
    "test:jasmine:es2015": "ng test --ts-config ./tsconfig.es2015.spec.json --progress=false",
    "test:jasmine:debug": "ng test -- --watch --browsers Chrome",
    "test:jest": "npm run test:jest:es5 && npm run test:jest:es2015",
    "test:jest:es5": "jest -i --config ./jest.es5.js",
    "test:jest:es2015": "jest -i --config ./jest.es2015.js",
    "test:jest:debug": "jest -i --watch"
  },
  "peerDependencies": {
    "ng-mocks": "*"
  },
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "core-js": "2.5.7",
    "rxjs": "6.2.2",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.8.9",
    "@angular/cli": "6.2.9",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@types/jasmine": "2.8.20",
    "@types/jest": "23.3.14",
    "@types/node": "8.9.5",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "23.6.0",
    "jest-preset-angular": "6.0.2",
    "karma": "5.2.3",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-packagr": "5.7.1",
    "npm": "6.14.16",
    "puppeteer": "2.1.1",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "typescript": "2.9.2"
  },
  "engines": {
    "npm": "6.14.16"
  }
}
