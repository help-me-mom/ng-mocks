{
  "name": "a14",
  "description": "Angular 14",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "npm": "npm",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "npm run test:jasmine && npm run test:jest",
    "test:debug": "npm run test:jasmine:es2015 -- --browsers=Chrome --watch",
    "test:jasmine": "npm run test:jasmine:es5 && npm run test:jasmine:es2015",
    "test:jasmine:es5": "ng test --ts-config ./tsconfig.es5ivy.spec.json --progress=false",
    "test:jasmine:es2015": "ng test --ts-config ./tsconfig.es2015ivy.spec.json --progress=false",
    "test:jasmine:debug": "ng test -- --watch --browsers Chrome",
    "test:jest": "npm run test:jest:es5 && npm run test:jest:es2015",
    "test:jest:es5": "jest -i --config jest.es5ivy.js",
    "test:jest:es2015": "jest -i --config jest.es2015ivy.js",
    "test:jest:debug": "jest -i --watch"
  },
  "peerDependencies": {
    "ng-mocks": "*"
  },
  "dependencies": {
    "@angular/animations": "14.0.0-next.13",
    "@angular/common": "14.0.0-next.13",
    "@angular/compiler": "14.0.0-next.13",
    "@angular/core": "14.0.0-next.13",
    "@angular/forms": "14.0.0-next.13",
    "@angular/platform-browser": "14.0.0-next.13",
    "@angular/platform-browser-dynamic": "14.0.0-next.13",
    "@angular/router": "14.0.0-next.13",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.0-next.13",
    "@angular/cli": "14.0.0-next.13",
    "@angular/compiler-cli": "14.0.0-next.13",
    "@types/jasmine": "3.10.6",
    "@types/jest": "27.5.0",
    "@types/node": "16.11.33",
    "jasmine-core": "4.0.1",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.2",
    "karma": "6.3.19",
    "karma-chrome-launcher": "3.1.1",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "npm": "8.8.0",
    "puppeteer": "13.7.0",
    "typescript": "4.6.4"
  },
  "engines": {
    "npm": "8.8.0"
  }
}
